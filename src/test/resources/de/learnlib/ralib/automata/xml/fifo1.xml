<?xml version="1.0" encoding="UTF-8" ?>
<register-automaton>
   <alphabet>
      <inputs>
         <symbol name="IGet"/>
         <symbol name="IPut"> 
            <param type="int" name="p0"/> 
         </symbol>
      </inputs>
      <outputs>
         <symbol name="OGet"> 
            <param type="int" name="p0"/>
         </symbol>
         <symbol name="ONOK"/>
         <symbol name="OOK"/>
      </outputs>
   </alphabet>
   <constants>
   </constants>
   <globals>
      <variable type="int" name="r0">0</variable>
      <variable type="int" name="out">0</variable>
   </globals>
   <locations>
      <!-- location name="ms"/ -->
      <location name="s0" initial="true"/>
      <location name="m0_1"/>
      <location name="m1_0"/>
      <location name="s1"/>
      <!-- location name="me"/ -->
   </locations>
   <transitions>
      <!-- transition from="s0" to="ms" symbol="IGet" / -->
      <!-- transition from="ms" to="s0" symbol="ONOK" / -->
      <transition from="s0" to="m0_1" symbol="IPut" params="p">
         <assignments>
            <assign to="r0">p</assign>
         </assignments>
      </transition>
      <transition from="m0_1" to="s1" symbol="OOK" />
      <transition from="m1_0" to="s0" symbol="OGet" params="r0">
         <!-- assignments>
            <assign to="out">r0</assign>
         </assignments -->
      </transition>
      <transition from="s1" to="m1_0" symbol="IGet" />
      <!-- transition from="s1" to="me" symbol="IPut" params="p"/ -->
      <!--transition from="me" to="s1" symbol="ONOK" /-->
   </transitions>
</register-automaton>
