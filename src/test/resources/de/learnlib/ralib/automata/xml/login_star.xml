         </symbol>
         <symbol name="ILogin"> 
            <param type="int" name="p0"/>
            <param type="int" name="p1"/> 
         </symbol>
      </inputs>
      <outputs>
         <symbol name="ONOK"/>
         <symbol name="OOK"/>
         <symbol name="ANY"/>         
      </outputs>
   </alphabet>
   <constants>
   </constants>
   <globals>
   </globals>
